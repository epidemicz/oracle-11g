export ORACLE_BASE=/opt/oracle/app
export ORACLE_SID=orcl
export ORACLE_HOME=$ORACLE_BASE/product/11.2.0/dbhome_1
export ORACLE_INVENTORY=/opt/oracle/oraInventory
